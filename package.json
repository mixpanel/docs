    "url": "https://github.com/mixpanel/docs/issues"
  },
  "homepage": "https://github.com/mixpanel/docs#readme",
  "dependencies": {
    "@docsearch/js": "^3.6.1",
    "@docsearch/react": "^3.6.1",
    "clsx": "^2.1.1",
    "next": "^14.2.8",
    "nextra": "3.0.3",
    "nextra-theme-docs": "3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.78.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@redocly/openapi-cli": "1.0.0-beta.16",
    "@types/mixpanel": "^2.14.9",
    "@types/node": "22.5.2",
    "autoprefixer": "^10.4.20",
    "cspell": "^8.14.2",
    "postcss": "^8.4.45",
    "rdme": "8.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "yaml": "2.5.0"
  }
